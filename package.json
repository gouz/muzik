{
  "name": "muzik",
  "version": "2.1.2",
  "description": "Like a blog, but with a list of songs I like",
  "source": [
    "src/index.pug"
  ],
  "scripts": {
    "preparcel": "rm -rf dist docs .parcel-cache",
    "parcel": "parcel $RUN",
    "build": "RUN=build npm run parcel -- --no-cache --no-source-maps --dist-dir docs --public-url \"./\"",
    "watch": "RUN=watch npm run parcel",
    "dev": "RUN=serve npm run parcel -- --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gouz/muzik.git"
  },
  "author": "Sylvain Gougouzian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gouz/muzik/issues"
  },
  "homepage": "https://github.com/gouz/muzik#readme",
  "devDependencies": {
    "@parcel/compressor-brotli": "2.4.1",
    "@parcel/compressor-gzip": "2.4.1",
    "@parcel/optimizer-css": "2.4.1",
    "@parcel/packager-raw-url": "2.4.1",
    "@parcel/packager-xml": "2.4.1",
    "@parcel/transformer-css-experimental": "2.3.2",
    "@parcel/transformer-less": "2.4.1",
    "@parcel/transformer-pug": "2.4.1",
    "@prettier/plugin-pug": "1.20.1",
    "eslint": "8.12.0",
    "parcel": "2.4.1",
    "parcel-config-pwa-manifest": "0.1.1",
    "parcel-reporter-static-files-copy": "1.3.4",
    "prettier": "2.6.2",
    "serve-static": "1.15.0",
    "tailwindcss": "3.0.24"
  },
  "dependencies": {
    "@parcel/service-worker": "2.4.1",
    "sha1": "1.1.1"
  },
  "pwaManifest": {
    "name": "Muzik",
    "shortName": "Muzik",
    "startURL": "/muzik/",
    "theme": "#f1f5f9",
    "display": "fullscreen",
    "generateIconOptions": {
      "baseIcon": "./src/img/favicon.svg",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true,
      "purpose": [
        "maskable",
        "any"
      ]
    }
  }
}
