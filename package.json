{
  "name": "muzik",
  "version": "1.9.0",
  "description": "Like a blog, but with a list of songs I like",
  "source": [
    "src/index.pug",
    "src/404.pug"
  ],
  "scripts": {
    "preparcel": "rm -rf dist docs .parcel-cache",
    "parcel": "parcel $RUN",
    "build": "RUN=build npm run parcel -- --no-cache --no-source-maps --dist-dir docs --public-url \"./\"",
    "watch": "RUN=watch npm run parcel",
    "dev": "RUN=serve npm run parcel -- --open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gouz/muzik.git"
  },
  "author": "Sylvain Gougouzian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gouz/muzik/issues"
  },
  "homepage": "https://github.com/gouz/muzik#readme",
  "devDependencies": {
    "@parcel/compressor-brotli": "2.2.0",
    "@parcel/compressor-gzip": "2.2.0",
    "@parcel/transformer-less": "2.2.0",
    "@parcel/transformer-pug": "2.1.1",
    "@prettier/plugin-pug": "1.19.1",
    "autoprefixer": "10.4.2",
    "cssnano-preset-advanced": "5.1.10",
    "parcel": "2.1.1",
    "parcel-reporter-static-files-copy": "1.3.4",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "serve-static": "1.14.2",
    "tailwindcss": "3.0.13"
  }
}
