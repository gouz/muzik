{
  "name": "muzik",
  "version": "2.1.2",
  "description": "Like a blog, but with a list of songs I like",
  "source": [
    "src/index.pug"
  ],
  "scripts": {
    "preparcel": "rm -rf dist docs .parcel-cache",
    "parcel": "parcel $RUN",
    "build": "RUN=build npm run parcel -- --no-cache --no-source-maps --dist-dir docs --public-url \"./\"",
    "watch": "RUN=watch npm run parcel",
    "dev": "RUN=serve npm run parcel -- --open --https"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gouz/muzik.git"
  },
  "author": "Sylvain Gougouzian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gouz/muzik/issues"
  },
  "homepage": "https://github.com/gouz/muzik#readme",
  "devDependencies": {
    "@parcel/compressor-brotli": "2.3.1",
    "@parcel/compressor-gzip": "2.3.1",
    "@parcel/optimizer-css": "2.3.1",
    "@parcel/transformer-css-experimental": "2.3.1",
    "@parcel/transformer-less": "2.3.0",
    "@parcel/transformer-pug": "2.3.0",
    "@prettier/plugin-pug": "1.19.2",
    "eslint": "8.8.0",
    "parcel": "2.3.1",
    "parcel-reporter-static-files-copy": "1.3.4",
    "prettier": "2.5.1",
    "serve-static": "1.14.2",
    "tailwindcss": "3.0.19"
  }
}
